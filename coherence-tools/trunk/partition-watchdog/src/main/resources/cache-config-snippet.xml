<?xml version="1.0"?>
<!DOCTYPE cache-config SYSTEM "cache-config.dtd">

<cache-config>

    <caching-scheme-mapping>
        <cache-mapping>
            <!-- 
                This is special configuration element for data loss monitor.
                Do not modify.
             -->
            <cache-name>CANARY_CACHE</cache-name>
            <scheme-name>CANARY_SCHEME</scheme-name>
        </cache-mapping>
    </caching-scheme-mapping>

    <caching-schemes>
        <distributed-scheme>
            <!-- 
                This is special configuration element for data loss monitor.
                Do not modify.
             -->
            <scheme-name>CANARY_SCHEME</scheme-name>
            <backing-map-scheme>
                <local-scheme/>
            </backing-map-scheme>
        </distributed-scheme>
    </caching-schemes>

</cache-config>